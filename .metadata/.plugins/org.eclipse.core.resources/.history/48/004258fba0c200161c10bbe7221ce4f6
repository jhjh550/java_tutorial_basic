public class ExceptionTest {
	public static void main(String[] args) {
		try{
			int a = 100/0;
			System.out.println("a:"+a);
		}catch(Exception e){
//			e.printStackTrace();
		}
		
		try {
			int a = 100/0;
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		System.out.println("end");
	}
}
