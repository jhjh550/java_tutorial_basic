import java.util.HashMap;

public class MapTest {

	public static void main(String[] args) {
		HashMap<String, Integer> map 
			= new HashMap<String, Integer>();
		map.put("Math", 100);
		map.put("English", 80);
		map.put("Math", 90);
		
		int mathValue = map.get("Math");
		System.out.println("Math:"+mathValue);

	}

}
